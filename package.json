{
  "name": "odin-members-only",
  "version": "0.0.0",
  "engines": {
    "node": ">=20.9.0"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "npm run build:css && nodemon ./bin/www",
    "serverstart": "DEBUG= <project-name> :* npm run devstart",
    "build:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css --watch",
    "watch:css": "npm run build:css --watch",
    "dev": "concurrently \"nodemon ./bin/www\" \"npm:build:css\" "
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "luxon": "^3.4.4",
    "mongoose": "^8.0.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "postcss-cli": "^11.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6"
  }
}
